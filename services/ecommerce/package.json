{
  "name": "@platform/ecommerce",
  "version": "1.0.0",
  "description": "Multi-vendor ecommerce marketplace service",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "dev:fastify": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:coverage": "vitest --run --coverage",
    "lint": "eslint src --ext .ts --fix",
    "lint:check": "eslint src --ext .ts",
    "type-check": "tsc --noEmit",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "tsx src/seed-simple.ts",
    "db:seed-original": "tsx src/seed.ts",
    "db:seed-comprehensive": "tsx src/seed-comprehensive.ts",
    "db:setup": "pnpm run db:generate && pnpm run db:migrate && pnpm run db:seed && pnpm run db:seed-comprehensive"
  },
  "dependencies": {
    "@fastify/cors": "^8.4.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/redis": "^6.1.1",
    "@fastify/swagger": "^8.12.0",
    "@fastify/swagger-ui": "^2.1.0",
    "@platform/types": "workspace:*",
    "@platform/supabase-client": "workspace:*",
    "@prisma/client": "^5.7.0",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.9",
    "@types/express-fileupload": "^1.5.1",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/uuid": "^11.0.0",
    "axios": "^1.12.2",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-rate-limit": "^8.1.0",
    "fastify": "^4.24.3",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "pino": "^8.16.2",
    "pino-pretty": "^10.2.3",
    "redis": "^4.6.12",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^13.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/redis": "^4.0.11",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@vitest/coverage-v8": "^1.0.0",
    "eslint": "^8.55.0",
    "prisma": "^5.7.0",
    "redis-memory-server": "^0.10.0",
    "supertest": "^6.3.4",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  }
}
