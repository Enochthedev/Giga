{
  "name": "@platform/upload",
  "version": "1.0.0",
  "description": "Centralized file upload and media management service",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:unit": "vitest --run src/__tests__/unit src/__tests__/services",
    "test:integration": "vitest --run src/__tests__/integration",
    "test:e2e": "vitest --run src/__tests__/e2e",
    "test:performance": "vitest --run src/__tests__/performance",
    "test:security": "vitest --run src/__tests__/security",
    "test:coverage": "vitest --run --coverage",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "lint": "eslint src --ext .ts --fix",
    "lint:check": "eslint src --ext .ts",
    "type-check": "tsc --noEmit",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "tsx src/seed.ts"
  },
  "dependencies": {
    "@fastify/cors": "^8.4.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/multipart": "^8.0.0",
    "@fastify/swagger": "^8.12.0",
    "@fastify/swagger-ui": "^2.1.0",
    "@platform/types": "workspace:*",
    "@prisma/client": "^5.22.0",
    "axios": "^1.6.2",
    "bull": "^4.12.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "fastify": "^4.24.3",
    "form-data": "^4.0.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.16.3",
    "pino": "^8.16.2",
    "pino-pretty": "^10.2.3",
    "prom-client": "^15.1.3",
    "redis": "^4.6.12",
    "sharp": "^0.33.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bull": "^4.10.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/form-data": "^2.5.2",
    "@types/ioredis": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.0",
    "@types/pg": "^8.10.9",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^9.0.8",
    "eslint": "^8.55.0",
    "prisma": "^5.7.0",
    "supertest": "^6.3.4",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  }
}